{
  "name": "rivalsreactoverlay",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/charts": "^2.2.7",
    "@ant-design/icons": "^6.0.0",
    "@reduxjs/toolkit": "^2.6.1",
    "antd": "^5.24.6",
    "cra-template-overwolf-typescript-redux": "3.2.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "i18next": "^23.16.8",
    "overwolf-hooks": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^9.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "encrypt-env": "node scripts/encrypt-env.js",
    "prestart": "npm run encrypt-env",
    "prebuild": "npm run encrypt-env",
    "start-remote-server": "redux-devtools --hostname=localhost --port=8081 --open",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "pre-build": "node scripts/update-manifest.js && npm run encrypt-env",
    "post-build": "node scripts/create-production-overwolf-build",
    "package-overwolfog": "cd build && tar -acf ../package.v$npm_package_version.zip * && cd -",
    "package-overwolf": "pushd build && tar -acf ../package.v$npm_package_version.zip * && popd",
    "build:overwolf": "npm run pre-build && npm run build && npm run post-build && npm run package-overwolf",
    "watch": "nodemon --watch src/screens --ext ts,tsx,json,css --exec \"npm run build:overwolf\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@overwolf/types": "^3.24.7",
    "@redux-devtools/cli": "^4.0.2",
    "@redux-devtools/remote": "^0.9.5",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.28",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@types/react-redux": "^7.1.34",
    "@types/remote-redux-devtools": "^0.5.8",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "semantic-release": "^22.0.12",
    "typescript": "^5.8.2"
  },
  "homepage": "."
}
